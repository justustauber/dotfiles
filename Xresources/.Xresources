rofi.font: DejaVu Sans Mono 8
rofi.lines: 10
!rofi.width: 28
!!rofi.hide-scrollbar: true
!!rofi.color-window: #242321, #e0e2e0
!!rofi.color-normal: #242321, #e0e2e0, #242321, #dc8b92, #ffffff
!!rofi.color-active: #242321, #e0e2e0, #242321, #dc8b92, #e0e2e0
!!rofi.color-urgent: #242321, #ffa724, #242321, #dc8b92, #e0e2e0
!rofi.color-enabled: true
!rofi.color-window: #393939, #393939, #268bd2
!rofi.color-normal: #393939, #ffffff, #393939, #268bd2, #ffffff
!rofi.color-active: #393939, #268bd2, #393939, #268bd2, #205171
!rofi.color-urgent: #393939, #f3843d, #393939, #268bd2, #ffc39c
!! Separator style (none, dash, solid)
rofi.separator-style: dash

URxvt.font: xft:DejaVu Sans Mono for Powerline:size=8
!URxvt.letterSpace: -1

URxvt.scrollBar: false

URxvt.perl-ext-common: default,matcher,selection-to-clipboard,url-select,resize-font
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.url-select.underline: true

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
URxvt.iso14755: false
URxvt.iso14755_52: false



